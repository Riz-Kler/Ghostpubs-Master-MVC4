<head prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb#">
    <title>@ViewBag.Title</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <link rel="canonical" href="@ViewBag.Link" />
    <link rel="publisher" href="https://plus.google.com/112746078414329366542/" />

    <meta property="og:locale" content="en_GB" />
    <meta property="og:type" content="article" />
    <meta property="og:title" content="@ViewBag.Title" />
    <meta property="og:image" content="http://www.ghostpubs.com/haunted-pubs/favicon.ico" />
    <meta property="og:description" content="@ViewBag.MetaDescription" />
    <meta property="og:url" content="@ViewBag.Link" />
    <meta property="og:site_name" content="Ghost Pubs" />

    <meta property="article:publisher" content="http://www.facebook.com/ghostpubspage" />

    @if (ViewBag.Tag != null)
    {
        <meta property="article:tag" content="@ViewBag.Tag" />
    }
    @if (ViewBag.Tags != null)
    {
        foreach (string tag in ViewBag.Tags)
        {
            <meta property="article:section" content="@tag" />
        }
    }

    <meta property="article:published_time" content="@DateTime.Now.ToLongDateString()" />
    <meta property="article:modified_time" content="@DateTime.Now.ToLongDateString()" />

    <meta property="fb:admins" content="100001874427547" />
    <meta property="fb:app_id" content="303827419746289" />

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

